{
	"name": "oh-my-gtd",
	"version": "0.0.46",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"migrations": "npx prisma migrate deploy",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "npm run test:integration:ci && npm run test:unit",
		"test:unit": "vitest",
		"test:integration:ci": "npx prisma migrate deploy && playwright test",
		"test:integration:local": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test playwright test",
		"test:integration:local:ui": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test npx -- playwright test --ui"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bcrypt": "^5.0.2",
		"@types/eslint": "8.56.0",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/pg": "^8.10.9",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.8.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.8.0",
		"@sentry/sveltekit": "^7.92.0",
		"bcrypt": "^5.1.1",
		"dayjs": "^1.11.10",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.11.3",
		"svelte-select": "^5.8.3",
		"zod": "^3.22.4"
	}
}
